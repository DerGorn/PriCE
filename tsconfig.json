{
  "compilerOptions": {
    "outDir": "dst",
    "removeComments": true,
    "target": "es2017",
    "moduleResolution": "node",
    "alwaysStrict": true,
    "allowUnreachableCode": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["./src/*.ts"]
}
